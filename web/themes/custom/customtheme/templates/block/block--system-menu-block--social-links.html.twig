{% set social_list = [] %}
{% set items = content['#items'] %}
{% for key, item in items %}
  {% set image_url = item.entity.field_social_network_images.entity.field_media_image.entity.fileuri %}
  {% set image_uri = file_url(image_url) %}
  {% set social_image = { 'image': image_uri, 'url': item.url } %}
  {% set social_list = social_list|merge([social_image]) %}
{% endfor %}

{% include '@molecules/menus/social-menu/social-menu.twig' with {
  social_menu_items: social_list
} %}
